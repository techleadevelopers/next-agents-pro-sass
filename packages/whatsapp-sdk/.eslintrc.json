{
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "semi": ["error", "never"],
      "quotes": ["error", "single"]
    }
  }
  